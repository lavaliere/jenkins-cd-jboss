<arquillian xmlns="http://jboss.org/schema/arquillian" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jboss.org/schema/arquillian <a href="http://jboss.org/schema/arquillian/arquillian_1_0.xsd">http://jboss.org/schema/arquillian/arquillian_1_0.xsd</a>">
    <defaultProtocol type="Servlet 3.0" />
    <container qualifier="arq-jbossas-managed" default="true">
        <engine>
            <property name="deploymentExportPath">target/</property>
        </engine>
        <configuration>
            <property name="jbossHome">/data/var/lib/jenkins/api-team/wildfly-8.1.0.Final</property>
        </configuration>
    </container>
</arquillian>
